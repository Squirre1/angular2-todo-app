{
  "compilerOptions": {
    "declaration": false,
    "baseUrl": ".",
    "paths": {
      "*": [
        "app/*",
        "node_modules/*"
      ]
    },
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "mapRoot": "./",
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true
  }
}
